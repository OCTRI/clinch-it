
{{>layout/header}}
<div class="container">

	<h1>{{entityName}} List</h1>
	{{>components/messages}}
	<table class='table table-striped table-bordered'>
		<thead>
		<tr>
			<th>Name</th>
			<th>Date of birth</th>
			<th>Actions</th>
		</tr>
		</thead>
		<tbody>
		{{#entity_list}}
		<tr>
			<td>{{#name}}{{.}}{{/name}}</th>
			<td>{{#dateOfBirth}}{{.}}{{/dateOfBirth}}</th>
			<td><a href='{{req.contextPath}}{{baseRoute}}/{{id}}/sdh'>View/Edit</a></td>
		</tr>
		{{/entity_list}}
		</tbody>
	</table>
</div>
{{>layout/footer}}